{
    "extends": "airbnb-base",
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true,
        "jest": true
    },
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": [
            "error",
            "never"
        ],
        "no-console": "off",
        "no-tabs": "off",
        "no-throw-literal": "off"
    }
}
